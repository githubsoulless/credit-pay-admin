 	 select a.* from (
  	select t.*,
  	ag.agent_id,
  	ag.agent_name,
  	b.name as chnlName, 
  	(case 
  		when t.order_tp=0 then p.fee
		when t.order_tp=1 then (select df_fee/pay_num from t_pay_plan where plan_id=t.plan_id)
		when t.order_tp=2 then (select fee from t_tx_record where record_id=t.busi_order_no)
		end
  	 )as platFee,
  	p.user_profits as userProfits,
  	p.agent_profits as agentProfits 
  	from t_order t 
  	left join t_pay_channel b on t.chnl_id=b.id 
  	left join t_pay_profits p on p.plan_id=t.plan_id and p.task_id=t.task_id
  	left join t_app_user au on au.user_id=t.user_id 
  	left join t_agent_inf ag on ag.agent_id=au.agent_id 
  	<include refid="where_page" />
  	order by t.create_time desc
  	)a limit  #{startRow} , #{pageSize}
  </select>
</mapper>