   <select id="getOrderByPageCount" parameterType="net.chrone.creditpay.model.Order" resultType="map">
  	select 
  		count(1) as count,
  		sum(t.amount)as sumamt, 
  		sum(t.fee) as sumfee ,
  		sum(case 
  		when t.order_tp=0 then p.fee
		when t.order_tp=1 then (select CAST(df_fee/pay_num  AS SIGNED integer)  from t_pay_plan where plan_id=t.plan_id)
		when t.order_tp=2 then (select fee from t_tx_record where record_id=t.busi_order_no)
		end
	  	 )as sumPlatFee,
	  	sum(p.user_profits) as sumUserProfits,
	  	sum(p.agent_profits) as sumAgentProfits 
  		from t_order t 
  		left join t_pay_channel b on t.chnl_id=b.id 
  		left join t_pay_profits p on p.plan_id=t.plan_id and p.task_id=t.task_id
  		left join t_app_user au on au.user_id=t.user_id 
  		left join t_agent_inf ag on ag.agent_id=au.agent_id 
  	<include refid="where_page" />
  </select>
